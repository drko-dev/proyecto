{% spaceless %}
     
<!DOCTYPE html>
<html>

    {# HEAD #}
    <head>
        {% block head %}        
            <meta charset="UTF-8" />
            <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
            {% block stylesheets %}
                <link rel="stylesheet" href="{{ asset('build/css/global.css') }}">
            {% endblock %}
            <title>{% block title %}Proyecto{% endblock %}</title>
        {% endblock %}
    </head>
    {# FIN HEAD #}
    
    {# BODY #}
    <body>
        {% block body %}

        {# HEADER #}
        <header>
            {% block header %}{% endblock %}
        </header>    
        {# FIN HEADER #}
            
        {# NAV #}
        <nav>
            {% block nav %}
            <div class="container">
                <ul id="slide-out" class="side-nav">
                    <li>
                        <div class="user-view">
                            <div class="background">
                                <img src="https://previews.123rf.com/images/monsitj/monsitj1606/monsitj160600003/57574562-Programming-code-abstract-technology-background-of-software-developer-and-Computer-script-Stock-Photo.jpg">
                            </div>
                            <a href="#!user"><img class="circle" src="apple-touch-icon.png"></a>
                            <a href="#!name"><span class="white-text name">Cts Sistemas</span></a>
                            <a href="#!email"><span class="white-text email">contacto@ctssistemas.com</span></a>
                        </div>
                    </li>
                    <li><a class="waves-effect" href="#"><i class="material-icons">home</i>Inicio</a></li>
                    <li><a class="waves-effect" href="#">Novedades</a></li>
                    <li><a class="waves-effect" href="#">Nosotros</a></li>
                    <li><a class="waves-effect" href="#">Contacto</a></li>
                </ul>
            </div> 

            <div class="nav-wrapper white">
                <a href="#" data-activates="slide-out" class="button-collapse show-on-large white">
                    <i class="material-icons">menu</i>
                </a> 
                <img class="brand-logo center logo-margin" src="images/logo.png"/>
                <ul class="right hide-on-med-and-down">
                    <li><a href="#">Inicio</a></li>
                    <li><a href="#">Novedades</a></li>
                    <li><a href="#">Acerca</a></li>
                    <li><a href="#">Contacto</a></li>
                </ul>                    
            </div>
            {# ...or you can read and display every flash message available #}
            {% for label, messages in app.flashes %}
                {% for message in messages %}
                    <div class="flash-{{ label }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
            
            {% endblock %}
        </nav>
        {# FIN NAV #}

        {# MAIN #}
        <main>
            {% block main %}
            
            {# SECTION #}
            <section>
                {% block section %}
    
                    {# ARTICLE #}
                    <article>
                        {% block article %}{% endblock %}
                    </article>
                    {# FIN ARTICLE #}
                    
                {% endblock %}
            </section>
            {# FIN SECTION #}

            {# ASIDE #
            <aside>
                {% block aside %}{% endblock %}
            </aside>
            # FIN ASIDE #}

            {% endblock %}
        </main>
        {# FIN MAIN #}
        
        {# FOOTER #}
        <footer class="page-footer page-footer-top hide-on-small-only">
            {% block footer %}
{#                 <div class="container">
                    <div class="row">
                        <div class="col l6 s12">
                            <h5 class="white-text">Footer Content</h5>
                            <p class="grey-text text-lighten-4">You can use rows and columns here to organize your footer content.</p>
                        </div>
                        <div class="col l4 offset-l2 s12">
                            <h5 class="white-text">Links</h5>
                            <ul>
                                <li><a class="grey-text text-lighten-3" href="#!">Link 1</a></li>
                                <li><a class="grey-text text-lighten-3" href="#!">Link 2</a></li>
                            </ul>
                        </div>
                    </div>
                </div> #}
                <div class="footer-copyright">
                    <div class="container">
                        Â© 2017 Copyright Text
                        <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
                    </div>
                </div>
            {% endblock %}
        </footer>
        {# FIN FOOTER #}

        {# JAVASCRIPT #}
        {% block javascripts %}
            <script src="{{ asset('build/js/app.js') }}"></script>
        {% endblock %}
        {# FIN JAVASCRIPT #}
        
        {% endblock %}
    </body>
    {# FIN BODY #}

</html>
        
{% endspaceless %} 